{
  "name": "react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env VITE_ENV=dev vite",
    "start:backend": "json-server --watch db.json --port 5000 --delay 1000",
    "typegen": "npx openapicmd typegen ./src/apis/api.yml > src/apis/api.d.ts && prettier --write ./src/apis/api.d.ts",
    "build": "npm run typegen && tsc && cross-env VITE_ENV=prod vite build",
    "preview": "vite preview",
    "test": "cross-env VITE_ENV=test vitest",
    "test:updateSnapshots": "cross-env VITE_ENV=test vitest --update",
    "test:coverage": "cross-env VITE_ENV=test vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "type-check": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^6.1.2",
    "@mui/material": "^6.1.2",
    "@mui/x-date-pickers": "^7.1.1",
    "@reduxjs/toolkit": "^2.2.3",
    "ag-grid-community": "^32.2.1",
    "ag-grid-react": "^32.2.1",
    "axios": "^1.6.8",
    "date-fns": "^4.1.0",
    "formik": "^2.4.5",
    "js-yaml": "^4.1.0",
    "notistack": "^3.0.1",
    "openapi-client-axios": "^7.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.2",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.7.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^25.0.1",
    "json-server": "^0.17.4",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^2.1.2"
  }
}
