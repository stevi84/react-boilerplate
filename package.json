{
  "name": "react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env VITE_ENV=dev vite",
    "start:backend": "json-server --watch db.json --port 5000 --delay 1000",
    "typegen": "npx openapicmd typegen ./src/apis/api.yml > src/apis/api.d.ts && prettier --write ./src/apis/api.d.ts",
    "build": "npm run typegen && tsc && cross-env VITE_ENV=prod vite build",
    "preview": "vite preview",
    "test": "cross-env VITE_ENV=test vitest",
    "test:updateSnapshots": "cross-env VITE_ENV=test vitest --update",
    "test:coverage": "cross-env VITE_ENV=test vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "type-check": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.1",
    "@mui/material": "^5.15.1",
    "@mui/x-date-pickers": "^6.18.5",
    "@reduxjs/toolkit": "^2.0.1",
    "ag-grid-community": "^31.0.1",
    "ag-grid-react": "^31.0.1",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "formik": "^2.4.5",
    "js-yaml": "^4.1.0",
    "notistack": "^3.0.1",
    "openapi-client-axios": "^7.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.0",
    "jsdom": "^23.0.1",
    "json-server": "^0.17.4",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.1.0"
  }
}
