{
  "name": "react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env VITE_ENV=dev vite",
    "start:backend": "json-server --watch db.json --port 5000 --delay 1000",
    "typegen": "npx openapicmd typegen ./src/apis/api.yml > src/apis/api.d.ts && prettier --write ./src/apis/api.d.ts",
    "build": "npm run typegen && tsc && cross-env VITE_ENV=prod vite build",
    "preview": "vite preview",
    "test": "cross-env VITE_ENV=test vitest",
    "format": "prettier --write .",
    "lint": "eslint .",
    "type-check": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.17",
    "@mui/x-date-pickers": "^6.18.2",
    "@reduxjs/toolkit": "^1.8.1",
    "ag-grid-community": "^30.2.1",
    "ag-grid-react": "^30.2.1",
    "axios": "^1.6.1",
    "date-fns": "^2.30.0",
    "formik": "^2.4.5",
    "i18next-browser-languagedetector": "^7.2.0",
    "js-yaml": "^4.1.0",
    "notistack": "^3.0.1",
    "openapi-client-axios": "^7.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.4.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.18.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^21.1.0",
    "json-server": "^0.17.4",
    "prettier": "^2.7.1",
    "typescript": "^5.0.2",
    "vite": "^5.0.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.34.6"
  }
}
